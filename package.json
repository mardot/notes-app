{
  "name": "intro-reactrt",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "mongoose": "^5.13.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "node ./server/index.js",
    "dev": "nodemon index.js",
    "server": "json-server -p3001 --watch db.json",
    "build:ui": "rm -rf build && cd ./client/ && npm run build --prod && cp -r build ../server",
    "deploy": "git push --set-upstream origin herokudeploy",
    "deploy:full": "npm run build:ui && git checkout herokudeploy && git add . && git commit -m 'uibuild' && npm run deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "json-server": "^0.16.3"
  },
  "proxy": "http://localhost:3001"
}
